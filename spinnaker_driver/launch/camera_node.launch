<launch>
  <node pkg="spinnaker_driver" type="spinnaker_driver_node" name="spinnaker_driver_node" clear_params="true" output="screen">
    <rosparam command="load" file="$(find spinnaker_driver)/config/camera.yaml"/>

    <!-- Enable Dynamic Reconfigure -->
    <rosparam param="enable_dyn_reconf">false</rosparam>
  </node>
</launch>
